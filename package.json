{
  "name": "cordova-admob-sdk",
  "version": "0.24.1",
  "description": "Google AdMob SDK to Cordova.",
  "scripts": {
    "version": "sync-package-version --cordova-plugin"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.14",
    "lint-staged": "^7.3.0",
    "lintspaces-cli": "^0.6.1",
    "sync-package-version": "0.0.5"
  },
  "cordova": {
    "id": "cordova-admob-sdk",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": "rehy/cordova-admob-sdk",
  "keywords": [
    "cordova",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "ad",
    "admob",
    "google",
    "sdk"
  ],
  "author": "Ratson",
  "license": "MIT",
  "files": [
    "*.js",
    "*.md",
    "*.xml",
    "LICENSE",
    "src",
    "www"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "plugin.xml": [
      "lintspaces -d spaces -s 4"
    ]
  }
}
